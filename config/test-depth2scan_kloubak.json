{
  "version": 2,
  "robot": {
    "modules":
    {
      "depth2scan": {
          "driver": "app.depth2scan_kloubak:DepthToScan",
          "init": {}
      },
      "rs": {
          "driver": "realsense",
          "init": {
            "device": "D400",
            "depth_subsample": 1,
            "depth_resolution": [640, 360],
            "depth_fps": 30
          }
      }
  },
    "links": [
      ["rs.depth", "depth2scan.depth"]
    ]
  }
}
